{
	"name": "n8n-nodes-talentsight",
	"version": "0.1.0",
	"description": "TalentSight API integration for n8n",
	"license": "MIT",
	"homepage": "https://github.com/alexander-gekov/n8n-nodes-talentsight",
	"keywords": [
		"n8n-community-node-package",
		"talentsight",
		"n8n"
	],
	"author": {
		"name": "TalentSight",
		"email": "info@talentsight.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/alexander-gekov/n8n-nodes-talentsight.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/TalentSightApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TalentSight/TalentSight.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"n8n-workflow": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
